# ===============================================
# IDO Frontend Environment Variables
# ===============================================
# Copy this file to .env.local and fill in your values
# DO NOT commit .env.local to version control
# ===============================================

# Admin password for protected routes (/admin/*)
NEXT_PUBLIC_ADMIN_PASSWORD=your_admin_password_here

# Backend API URL
# Local development: http://localhost:1234
# HuggingFace Spaces: https://YOUR_USERNAME-ido-backend.hf.space
NEXT_PUBLIC_API_URL=http://localhost:8000
